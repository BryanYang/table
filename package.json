    "eslint": "^7.1.0",
    "father": "^2.22.0",
    "immutability-helper": "^3.0.0",
    "less": "^3.10.3",
    "np": "^6.0.0",
    "prettier": "^2.0.1",
    "rc-animate": "^3.0.0",
