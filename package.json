    "rc-checkbox": "^2.3.1",
    "rc-motion": "^2.4.1",
    "rc-resize-observer": "^0.2.0",
    "rc-select": "^11.5.1",
    "rc-tree": "^4.0.0-beta.2",
    "rc-util": "^5.0.4",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
